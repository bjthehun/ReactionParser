import "http://vitruv.tools/reactionsparser/model" as ascet

reactions: test2
in reaction to changes in ascet
execute actions in ascet

reaction TestReaction {
	after element ascet::Task deleted
	call doSomething(affectedEObject)
}

routine doSomething (ascet::Task task){
	// batch should be match
	// ANTLR finds this syntax error
	batch{
		val x = retrieve ascet::Task corresponding to task
	}
	// updat should be update
	// ANTLR misses this error
	updat{
		removeObject(x)
	}
}
